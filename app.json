{
  "expo": {
    "name": "expo-starter-kit",
    "slug": "expo-starter-kit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.viro.expo",
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) uses the camera for AR functionality"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.example.viro.expo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["@viro-community/react-viro"]
  }
}
